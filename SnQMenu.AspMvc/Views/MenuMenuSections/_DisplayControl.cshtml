@*@CustomerCode*@
@{
    @model SnQMenu.AspMvc.Models.IdentityModel

    var viewBagWrapper = Model.ViewBagInfo;
    var property = viewBagWrapper.DisplayProperty;
    var displayViewModel = viewBagWrapper.CreateDisplayViewModel(Model);

    @if (property.Name.Equals("ImageUrl"))
    {
        viewBagWrapper.Handled = true;
        <img src="@displayViewModel.GetDisplayValue(property)" alt="Item image">
    }
    else
    {
        @await Html.PartialAsync("../Shared/_DisplayControl", Model)
    }
}
